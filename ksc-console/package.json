{
  "name": "ksc-console",
  "version": "0.5.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode=mock",
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "build:live": "tsc && vite build --mode production",
    "serve": "vite preview",
    "swagger:dev": "cross-env NODE_ENV=dev node swagger-tap.mjs",
    "swagger:test": "cross-env NODE_ENV=test node swagger-tap.mjs",
    "swagger:live": "cross-env NODE_ENV=live node swagger-tap.mjs",
    "check-type": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "unit:test": "jest --env=jest-environment-jsdom",
    "eslint": "eslint 'src/**/*.ts?(x)'",
    "eslint:fix": "eslint --fix 'src/**/*.ts?(x)'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tap/core": "^0.3.0",
    "cron-parser": "^4.4.0",
    "dayjs": "^1.10.7",
    "diff": "^5.1.0",
    "echarts": "^5.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "infra-design-icons": "^4.7.14",
    "infrad": "^4.21.7",
    "isomorphic-fetch": "^3.0.0",
    "query-string": "^7.1.1",
    "ramda": "^0.27.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-google-login": "^5.2.2",
    "react-resize-detector": "^7.0.0",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-use": "^17.3.1",
    "recoil": "^0.5.2",
    "styled-components": "^5.3.3",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@infra-web-kit/eslint-config-infra-react": "^1.2.0",
    "@infra-web-kit/eslint-config-infra-typescript": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-react": "^1.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-environment-jsdom": "^27.4.3",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "path": "^0.12.7",
    "prettier": "^2.5.0",
    "prettier-eslint-cli": "^5.0.1",
    "request": "^2.88.2",
    "tapable": "^2.2.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "4.5.5",
    "vite": "^2.6.4",
    "vite-plugin-react-svg": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-type && lint-staged && yarn unit:test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
