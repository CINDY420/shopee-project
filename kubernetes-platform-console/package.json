  "scripts": {
    "dev:vite": "vite",
    "dev": "webpack-dev-server --colors --progress --mode development --env.server=dev --env.isLocal",
    "test": "webpack-dev-server --colors --progress --mode development --env.server=test --env.isLocal",
    "staging": "webpack-dev-server --colors --progress --mode development --env.server=staging --env.isLocal",
    "plat-test": "webpack-dev-server --colors --progress --mode development --env.server=plat-test",
    "unit:test": "jest --env=jest-environment-jsdom",
    "unit:ci": "jest --env=jest-environment-jsdom --cacheDirectory=/etc/ci_cache/jest_cache/kubernetes-platform-console",
    "build:dev": "webpack --colors --mode production --env.server=dev",
    "build:test": "webpack --colors --mode production --env.server=test",
    "build:plat-test": "webpack --colors --mode production --env.server=plat-test",
    "build:banking-test": "webpack --colors --mode production --env.server=banking-test",
    "build:banking-live": "webpack --colors --mode production --env.server=banking-live",
    "build:staging": "webpack --colors --mode production --env.server=staging",
    "build:live": "webpack --colors --mode production --env.server=live",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "lint-staged": "lint-staged",
    "eslint": "eslint 'src/**/*.ts?(x)'",
    "check-type": "tsc --noEmit",
    "styleguide:server": "styleguidist server --open",
    "styleguide:build": "styleguidist build",
    "build:tsdoc": "typedoc --disableOutputCheck --sourcefile-url-prefix http://localhost:6060/docs/",
    "build:doc": "npm run build:tsdoc && npm run styleguide:server",
    "start": "concurrently \"npm run dev\" \"npm run build:doc\"",
    "swagger:dev:v1": "NODE_ENV_API_VERSION=v1 yo swagger-tap --e=dev",
    "swagger:test:v1": "NODE_ENV_API_VERSION=v1 yo swagger-tap --e=test",
    "swagger:plat-test:v1": "NODE_ENV_API_VERSION=v1 yo swagger-tap --e=plat-test",
    "swagger:dev:v3": "NODE_ENV_API_VERSION=v3 yo swagger-tap --e=dev",
    "swagger:test:v3": "NODE_ENV_API_VERSION=v3 yo swagger-tap --e=test",
    "swagger:plat-test:v3": "NODE_ENV_API_VERSION=v3 yo swagger-tap --e=plat-test",
    "test-server": "webpack-dev-server --mode development --env.server=test --env.cypress=ci",
    "cypress:open-test": "cypress open --env cypressEnv=test",
    "cypress:open-dev": "cypress open  --env cypressEnv=dev",
    "cypress:run-dev": "cypress run --env cypressEnv=test",
    "cypress:run-ci": "cypress run --env cypressEnv=test --record --key 5ca7f9e8-00dc-47b3-b001-5dff7bb43753",
    "ui-test-dev": "start-server-and-test 'yarn test-server' http://localhost:4200 'yarn cypress:run-dev'",
    "ui-test-ci": "start-server-and-test 'yarn test-server' http://localhost:4200 'yarn cypress:run-ci'"
  },
