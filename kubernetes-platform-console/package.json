  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules",
      "cypress"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "__SERVER_ENV__": "dev",
      "__RELEASE__": "platform",
      "__IS_LOCAL__": false,
      "ts-jest": {
        "babelConfig": true
      }
    },
    "injectGlobals": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-type && lint-staged && yarn unit:test --coverage=false --cacheDirectory=/tmp/jest_cache/kubernetes-platform-console",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
